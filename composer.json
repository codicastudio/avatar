{
    "name": "codicastudio/avatar",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/avatar",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "illuminate/cache": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Avatar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Avatar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\Avatar\\ServiceProvider"
            ],
            "aliases": {
                "Avatar": "Laravolt\\Avatar\\Facade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
